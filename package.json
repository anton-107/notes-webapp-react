{
  "name": "notes-webapp-react",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "webpack --config webpack.config.mjs --env file=.env.example",
    "start": "webpack serve --env file=.env.development",
    "start-vite": "vite serve",
    "test": "jest -c jest.config.js",
    "test:coverage": "jest  -c jest.config.js --coverage",
    "jest-update-snapshots": "jest  -c jest.config.js --updateSnapshot",
    "check-format": "prettier --check .",
    "fix-format": "prettier --write .",
    "check-dependencies": "depcruise --config .dependency-cruiser.js src",
    "check-duplications": "jscpd --pattern 'src/**/*.*' -l 2 --exitCode 1",
    "lint": "eslint . --ext .ts,.tsx",
    "fix-lint": "eslint . --ext .ts,.tsx --fix",
    "fix": "npm run fix-format && npm run fix-lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anton-107/notes-webapp-react.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/anton-107/notes-webapp-react/issues"
  },
  "homepage": "https://github.com/anton-107/notes-webapp-react#readme",
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^28.1.6",
    "@types/react": "^18.0.15",
    "@types/react-beautiful-dnd": "^13.1.2",
    "@types/react-dom": "^18.0.6",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@vitejs/plugin-react": "^3.0.0",
    "css-loader": "^6.7.1",
    "dependency-cruiser": "^12.1.1",
    "dotenv-webpack": "^8.0.0",
    "eslint": "^8.21.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "jest-fetch-mock": "^3.0.3",
    "jscpd": "^3.5.3",
    "prettier": "^2.7.1",
    "react-test-renderer": "^18.2.0",
    "style-loader": "^3.3.1",
    "ts-jest": "^28.0.7",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "vite": "^4.0.3",
    "vite-plugin-html": "^3.2.0",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3"
  },
  "dependencies": {
